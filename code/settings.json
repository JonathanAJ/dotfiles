{
    "window.zoomLevel": -1,
    "git.confirmSync": false,
    "git.autofetch": true,
    "workbench.colorTheme": "Dracula Soft",
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "editor.fontFamily": "'DejaVu Sans Mono', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.fontLigatures": "'PowerlineSymbols'",
    "workbench.startupEditor": "newUntitledFile",
    "terminal.explorerKind": "external",
    "[dart]": {
        "editor.formatOnSave": true,
        "editor.formatOnType": true,
        "editor.rulers": [
            80
        ],
        "editor.selectionHighlight": false,
        "editor.suggest.snippetsPreventQuickSuggestions": false,
        "editor.suggestSelection": "first",
        "editor.tabCompletion": "onlySnippets",
        "editor.wordBasedSuggestions": false
    },
    "files.associations": {
        "*.dart": "dart",
        "*.md": "markdown"
    },
    "dart.debugExternalLibraries": false,
    "dart.debugSdkLibraries": false,
    "terminal.integrated.fontFamily": "FontAwesome, monospace",
    "workbench.iconTheme": "vscode-icons",
    "security.workspace.trust.untrustedFiles": "open",
    "vim.vimrc.enable": true,
    "vim.handleKeys": {
        "<C-f>": false,
        "<C-Tab>": false, 
        "<C-b>": false, 
        "<C-d>": false, 
        "<C-w>": false,
    },
    "explorer.confirmDragAndDrop": false,
    "explorer.confirmDelete": false,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "vsicons.dontShowNewVersionMessage": true,
    "editor.stickyScroll.enabled": true,
    "maven.executable.path": "/opt/apache-maven-3.6.0/bin/mvn"
}